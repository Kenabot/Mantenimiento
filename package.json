{
  "name": "mantenimiento",
  "version": "0.2.7-dev",
  "description": "Simple bot de Discord para anunciar un mantenimiento en varios clientes",
  "main": "dist/main.js",
  "private": false,
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only --trace-deprecation --trace-warnings --poll main.ts",
    "lint": "eslint .",
    "update": "yarn upgrade-interactive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kenabot/Mantenimiento.git"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/holasoyender"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript"
  ],
  "author": "holasoyender",
  "license": "Apache-2.0",
  "packageManager": "yarn@3.2.2",
  "engines": {
    "node": ">= 17.1.0",
    "npm": ">= 8.5.0"
  },
  "os": [
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "x86",
    "arm64"
  ],
  "bugs": {
    "url": "https://github.com/Kenabot/Mantenimiento/issues"
  },
  "homepage": "https://kenabot.xyz",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "discord.js-light": "^4.9.0"
  }
}